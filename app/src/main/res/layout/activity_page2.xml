<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ECECEC"
    tools:context=".Pages.Page2">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="592dp"
        android:layout_gravity="center">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/textView"
                android:layout_width="137dp"
                android:layout_height="wrap_content"
                android:text="Calculo de Peso de Bovinos"
                android:textAlignment="viewStart"
                android:textSize="18sp"
                android:textStyle="bold"
                tools:ignore="HardcodedText,TextSizeCheck"
                tools:layout_editor_absoluteX="50dp"
                tools:layout_editor_absoluteY="50dp" />

            <Button
                android:id="@+id/btn1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="end"
                android:text="Ver"
                tools:ignore="DuplicateSpeakableTextCheck"
                tools:layout_editor_absoluteX="274dp"
                tools:layout_editor_absoluteY="50dp" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <TextView
            android:id="@+id/textView6"
            android:layout_width="137dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center|start"
            android:layout_marginLeft="50dp"
            android:layout_marginBottom="100dp"
            android:text="Calculo de Rendimiento del Canal Bovinos"
            android:textSize="18sp"
            android:textStyle="bold"
            tools:ignore="TextSizeCheck" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="137dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center|start"
            android:layout_marginLeft="50dp"
            android:layout_marginBottom="-5dp"
            android:text="AsignaciÃ³n de forraje"
            android:textSize="18sp"
            android:textStyle="bold"
            tools:ignore="TextSizeCheck" />

        <Button
            android:id="@+id/btn2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center|end"
            android:layout_marginRight="50dp"
            android:layout_marginBottom="100dp"
            android:text="Ver" />

        <Button
            android:id="@+id/btn3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical|end"
            android:layout_marginRight="50dp"
            android:text="Ver" />

    </FrameLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="59dp"
        android:background="#53BF35"
        android:orientation="horizontal"/>

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:layout_width="match_parent"
        android:layout_height="71dp"
        android:layout_gravity="bottom" >

        <ImageButton
            android:id="@+id/home_btn"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:layout_gravity="center|start"
            android:layout_marginLeft="30dp"
            android:layout_marginBottom="5dp"
            android:background="#FFFFFF"
            android:scaleType="fitCenter"
            app:srcCompat="@drawable/casa"
            tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom|center|start"
            android:layout_marginLeft="31dp"
            android:layout_marginBottom="5dp"
            android:text="Home"
            android:textSize="12sp" />

        <ImageButton
            android:id="@+id/imageButton3"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:layout_gravity="center|start"
            android:layout_marginLeft="130dp"
            android:layout_marginBottom="5dp"
            android:background="#FFFFFF"
            android:scaleType="fitCenter"
            app:srcCompat="@drawable/vacachica"
            tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom|center|end"
            android:layout_marginRight="10dp"
            android:layout_marginBottom="5dp"
            android:text="Notificaciones"
            android:textSize="12sp" />

        <ImageButton
            android:id="@+id/imgbtnManejo"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:layout_gravity="center|end"
            android:layout_marginRight="130dp"
            android:layout_marginBottom="5dp"
            android:background="#FFFFFF"
            android:scaleType="fitCenter"
            app:srcCompat="@drawable/barras"
            tools:ignore="ImageContrastCheck,SpeakableTextPresentCheck,TouchTargetSizeCheck" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom|center"
            android:layout_marginLeft="55dp"
            android:layout_marginBottom="5dp"
            android:text="Manejo"
            android:textSize="12sp" />

        <ImageButton
            android:id="@+id/imageButton5"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:layout_gravity="center|end"
            android:layout_marginEnd="30dp"
            android:layout_marginBottom="5dp"
            android:background="#FFFFFF"
            app:srcCompat="@drawable/campanas"
            tools:ignore="ImageContrastCheck,SpeakableTextPresentCheck,TouchTargetSizeCheck" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom|center"
            android:layout_marginRight="50dp"
            android:layout_marginBottom="5dp"
            android:text="Animales"
            android:textSize="12sp" />
    </com.google.android.material.bottomnavigation.BottomNavigationView>

</FrameLayout>